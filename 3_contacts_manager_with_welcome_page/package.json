{
  "name": "1_contacts_manager",
  "version": "1.0.0",
  "description": "MERN applicatin to manage contacts. Supports all CRUD operations and authentication using JWT.",
  "main": "index.js",
  "scripts": {
    "server": "nodemon server.js",
    "reactClient": "npm start --prefix client",
    "app": "concurrently \"npm run server\" \"npm run reactClient\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "react",
    "context",
    "MERN",
    "mongoDB",
    "express",
    "node",
    "react-router-dom",
    "react-bootstrap",
    "axios"
  ],
  "author": "garima",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "mongoose": "^5.9.6"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "nodemon": "^2.0.2"
  }
}
